# Database connection (Supabase-compatible)
DATABASE_URL=postgresql://user:password@host:port/database

# MLS Feed URLs (keep server-side only, never expose to client)
MLS_SF_FEED_URL=https://your-mls-url/singlefamily
MLS_CC_FEED_URL=https://your-mls-url/condo
MLS_MF_FEED_URL=https://your-mls-url/multifamily

# Optional: Resend API key to enable transactional email sending
RESEND_API_KEY=

# Public URL used in email links and redirects (used by server and client)
NEXT_PUBLIC_APP_URL=https://signals.namastebostonhomes.com

